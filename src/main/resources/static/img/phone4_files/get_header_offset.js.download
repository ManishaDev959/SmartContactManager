DT.onReady(()=>{const header=document.querySelector('.js-header');const headerSearch=document.querySelector('.js-header-search');const headerSearchForm=headerSearch?.querySelector('.js-header-search-form');DT.getHeaderOffset=()=>{if(header?.getBoundingClientRect().top>=0){return header.clientHeight}
if(headerSearch?.getBoundingClientRect().top>=0){return headerSearch.clientHeight}
if(headerSearchForm?.getBoundingClientRect().top>=0){return headerSearchForm.offsetHeight}
return 0}})